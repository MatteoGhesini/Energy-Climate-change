set fuel_production_fict(TECHNOLOGY);
*/RIV, SUN, WIN/
set secondary_production(TECHNOLOGY) / COCSPN2, COCHPH3, COSTPH1, COSTPH3, NGCCPH2, NGCHPH3, NGCHPN3, NGCSPN2, NGFCFH1, NGGCPH2, NGGCPN2, NGHPFH1, NGHPPH2/;


