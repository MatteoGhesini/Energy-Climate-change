set appliances(TECHNOLOGY) ;
set unmet_demand(TECHNOLOGY);
set primary_imports(TECHNOLOGY) /CO00I00, NG00I00/;
set secondary_imports(TECHNOLOGY) //;

set fuel_production(TECHNOLOGY) / CO00X00,  NG00X00/;
